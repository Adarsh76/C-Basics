#Pseudocode - Star Pattern - 2
Star Pyramid

N = 3
  * 
 ***
*****

read N
i=1
while(i<=N){
            spaces=1
            while(spaces<=N-i)
            {
              print " "
              spaces = spaces +1
             }
          
            stars = 1
            while(stars<=2*i -1)
            {
             print"*"
             stars = stars +1
           }
    print "\n"
    i = i + 1
   }
exit
   
